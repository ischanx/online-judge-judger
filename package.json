{
  "name": "my-midway-project",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "dependencies": {
    "@midwayjs/bootstrap": "^2.3.0",
    "@midwayjs/core": "^2.3.0",
    "@midwayjs/decorator": "^2.3.0",
    "@midwayjs/web": "^2.3.0",
    "egg": "^2.0.0",
    "egg-scripts": "^2.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@midwayjs/cli": "^1.2.95",
    "@midwayjs/egg-ts-helper": "^1.0.1",
    "@midwayjs/luckyeye": "^1.0.0",
    "@midwayjs/mock": "^2.3.0",
    "@types/ioredis": "^4.28.7",
    "@types/jest": "^26.0.10",
    "@types/node": "14",
    "cross-env": "^6.0.0",
    "husky": "^7.0.4",
    "jest": "^26.4.0",
    "lint-staged": "^12.3.4",
    "mwts": "^1.0.5",
    "ts-jest": "^26.2.0",
    "typescript": "^4.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "prepare": "husky install",
    "preinstall": "npx only-allow yarn",
    "start": "egg-scripts start --daemon --title=my-midway-project --framework=@midwayjs/web  --port=9001",
    "stop": "egg-scripts stop --title=my-midway-project",
    "start_build": "yarn build && cross-env NODE_ENV=development midway-bin dev",
    "dev": "cross-env ets && cross-env NODE_ENV=local midway-bin dev --ts --port=9001",
    "test": "midway-bin test --ts",
    "cov": "midway-bin cov --ts",
    "lint": "mwts check",
    "lint:fix": "mwts fix",
    "ci": "yarn cov",
    "build": "midway-bin build -c",
    "check": "luckyeye"
  },
  "midway-bin-clean": [
    ".vscode/.tsbuildinfo",
    "dist"
  ],
  "midway-luckyeye": {
    "packages": [
      "midway_v2"
    ]
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "",
  "license": "MIT",
  "lint-staged": {
    "*.{js,ts}": [
      "eslint --fix"
    ]
  }
}
